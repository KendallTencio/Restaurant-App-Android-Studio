<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aplicacionrestaurante">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Restaurante App"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ventanaConfiguraciones"
            android:parentActivityName=".MainActivity">

        </activity>
        <activity
            android:name=".realizarPedido"
            android:parentActivityName=".MainActivity"></activity>
        <activity
            android:name=".detallesDePedido"
            android:parentActivityName=".ventanaVisualizacionPedidos" />
        <activity
            android:name=".ventanaVisualizacionPedidos"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".pruebaFireBase"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ventanaSeleccionMesas"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>